{
  "name": "playwright-dev-finance",
  "version": "1.0.0",
  "description": "Project automation of training",
  "main": "index.js",
  "scripts": {
    "play:all": "npx playwright test --reporter=line,allure-playwright",
    "allure:clean": "if exist [[ allure-results ]] then rmdir /S /Q allure-results && mkdir allure-results fi",
    "allure:gener": "allure generate allure-results --clean -o allure-report",
    "allure:history": "if exist [[ allure-report/history ]] then move allure-report/history allure-results/history fi",
    "test": "npm-run-all allure:clean allure:gener play:all allure:history"
  },
  "keywords": [],
  "author": "Julio Santos",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.33.0",
    "allure-playwright": "^2.2.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "allure-commandline": "^2.22.0",
    "dotenv": "^16.0.3"
  }
}

{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:playwright/playwright-test"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {  
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "indent": [ "error", 2],
        "max-len": [ "error", 100 ]
    }
}
